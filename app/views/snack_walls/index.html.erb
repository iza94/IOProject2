<p id="notice"><%= notice %></p>

<h1>Listing Snack Walls</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @snack_walls.each do |snack_wall| %>
      <tr>
        <td><%= snack_wall.title %></td>
        <td><%= link_to 'Show', snack_wall %></td>
        <td><%= link_to 'Edit', edit_snack_wall_path(snack_wall) %></td>
        <td><%= link_to 'Destroy', snack_wall, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Snack wall', new_snack_wall_path %>
